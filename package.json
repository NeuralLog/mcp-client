{
  "name": "@ai-mcp-logger/mcp-client",
  "version": "0.1.0",
  "description": "Model Context Protocol client for AI-MCP-Logger integration with Claude",
  "main": "src/index.js",
  "type": "module",
  "bin": {
    "ai-mcp-logger-client": "./src/index.js"
  },
  "scripts": {
    "build": "tsc src/index.ts --esModuleInterop --target es2022 --module NodeNext --moduleResolution nodenext",
    "start": "node src/index.js",
    "dev": "npx tsx src/index.ts",
    "test": "jest",
    "docker:build": "docker build -t ai-mcp-logger-mcp-client .",
    "docker:run": "docker run -i -e WEB_SERVER_URL=http://localhost:3030 ai-mcp-logger-mcp-client",
    "docker:run:remote": "docker run -i -e WEB_SERVER_URL=http://your-server-address:3030 ai-mcp-logger-mcp-client"
  },
  "keywords": [
    "ai",
    "logger",
    "mcp",
    "claude",
    "anthropic"
  ],
  "author": "AI-MCP-Logger Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AI-MCP-Logger/mcp-client.git"
  },
  "bugs": {
    "url": "https://github.com/AI-MCP-Logger/mcp-client/issues"
  },
  "homepage": "https://github.com/AI-MCP-Logger/mcp-client#readme",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.9.0",
    "axios": "^1.8.4",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.10",
    "@types/node": "^20.17.30",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsx": "^4.19.3",
    "typescript": "^5.3.2"
  }
}
